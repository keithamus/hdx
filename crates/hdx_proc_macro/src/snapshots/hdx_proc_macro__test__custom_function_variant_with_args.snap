#[derive(Debug, Clone, PartialEq, Eq, PartialOrd, Ord, Hash)]
#[cfg_attr(feature = "serde", derive(serde::Serialize), serde())]
enum Foo {
    FitContent(::hdx_parser::T![Ident]),
    FitContentFunction(
        ::hdx_parser::T![Function],
        types::LengthPercentage,
        Option<::hdx_parser::T![')']>,
    ),
}
