impl<'a> ::hdx_parser::ToCursors<'a> for Foo {
    fn to_cursors(&self, s: &mut ::hdx_parser::CursorStream<'a>) {
        match self {
            Self::FitContent(inner) => {
                s.append(inner.into());
            }
            Self::FitContentFunction(function, val, close) => {
                s.append(function.into());
                ::hdx_parser::ToCursors::to_cursors(val, s);
                if let Some(close) = close {
                    s.append(close.into());
                }
            }
        }
    }
